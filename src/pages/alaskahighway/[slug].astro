---
import AlaskaHighwayLayout from "../../layouts/or.astro";
import { Debug } from "astro/components";
import data from '../../data/alaskaHighway.json'

console.log(data)

export function getStaticPaths() {
  console.log(data);
  let routes = [];
  for (const slug in data) {
    routes.push({params: { slug }});
  }
  return routes;
}

console.log(data);
const params = Astro.params as { slug: string };

let name = "";
for (const slug in data) {
  if (params.slug == slug) {
    name = data[slug];
  }
}
---

<AlaskaHighwayLayout name={name} slug={params.slug} />
